{
  "name": "prisma-template",
  "private": true,
  "description": "",
  "version": "0.0.0",
  "main": "app.js",
  "type": "module",
  "prisma": {
    "schema": "./src/db/schema.prisma",
    "seed": "node ./src/db/seed.js"
  },
  "scripts": {
    "test": "dotenv -e .env.test -- vitest watch",
    "start": "node ./src/bin/www.js",
    "dev": "dotenv -e .env.dev -- node --watch ./src/bin/www.js",
    "prod": "dotenv -e .env.prod -- node --watch ./src/bin/www.js",
    "server": "DEBUG=<project-name>:* npm run dev",
    "prisma": "npx prisma init",
    "seedDev": "dotenv -e .env.dev -- npx prisma db seed",
    "seedProd": "dotenv -e .env.prod -- npx prisma db seed",
    "generateDev": "dotenv -e .env.dev -- npx prisma generate",
    "generateProd": "dotenv -e .env.prod -- npx prisma generate",
    "migrateDev": "dotenv -e .env.dev -- npx prisma migrate dev",
    "migrateProd": "dotenv -e .env.prod -- npx prisma migrate deploy",
    "devReset": "dotenv -e .env.dev -- npx prisma migrate reset",
    "migrateCreateOnly": "dotenv -e .env.dev -- npx prisma migrate dev --create-only",
    "dbPush": "dotenv -e .env.dev -- npx prisma db push",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "update": "ncu -u -x eslint",
    "format": "prettier --write \"**/*.{js,mjs}\""
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "supertest": "^7.0.0",
    "vitest": "^3.0.5"
  }
}
